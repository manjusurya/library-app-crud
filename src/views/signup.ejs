        <div class="panel-body">
          
          
          
          <!--/stories-->
          
          <div class="container"> 
            <div class="row">
              
              <div class="col-md-4"></div>
              <div class="col-md-4">
               
                 
                  <form method="POST" action="/signup/login" onsubmit="return validate() "> 
                      
                          <tr>
                              <td>name:</td>
                              <td><input type="text" class="form-control" id="name"></td>
                          </tr>
                          <tr>
                              <td>D.O.B:</td>
                              <td><input type="date" class="form-control" id="dob"></td>
                          </tr>
                          <tr>
                              <td>address:</td>
                              <td><input type="text" class="form-control" id="address"></td>
                          </tr>
                          <tr>
                           <td>gender:</td>
                           <td>
                               <select class="form-control">
                                   <option value="male">male</option>
                                   <option value="female">female</option>
                                   <option value="other">other</option>
                               </select>
                           </td>
                         
                           
                       </tr>
                          <tr>
                              <td>email:</td>
                              <td><input type="email" class="form-control" id="email"></td>
                          </tr>
                          <tr>
                              <td>phone:</td>
                              <td><input type="number" class="form-control" id="phone"></td>
                          </tr>
                          <tr>
                              <td>password:</td>
                              <td><input type="password" class="form-control" id="password1"></td>
                              
                          </tr>
                          <tr>
                              <td>confirm password:</td>
                              <td><input type="password" class="form-control" id="password2"></td>
                          </tr>
                          <br>
                         
                          <tr>
                              <td></td>
                              <td><button type="submit" class="btn btn-primary" onsubmit="validate();">signup</button></td>
                              
                          </tr>
   
   
   
                  
                  </form>
              </div>
              <div class="col-md-4"></div>
           </div>
       </div>
   

          
         
          <!--/stories-->
          
          
